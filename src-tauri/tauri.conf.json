{
  "productName": "tafseel",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "$schema": "../../core/tauri-config-schema/schema.json",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tafseel",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": "event: default-src 'self'; connect-src ipc: http://ipc.localhost; default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
